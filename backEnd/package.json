{
  "name": "backEnd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "ts-node bin/www",
    "starts": "nodemon --watch bin/www",
    "build": "tsc",
    "debugger": "nodemon  --watch ./app -e  ts,tsx  --exec node --inspect -r ts-node/register  ./app/index.ts",
    "watch-server": "nodemon  --watch './src/**/*' -e  ts,tsx  --exec ts-node  bin/www"
  },
  "dependencies": {
    "@types/node": "^11.9.5",
    "debug": "^2.6.3",
    "glob": "^7.1.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.5.1",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "log4js": "^4.0.2",
    "mongoose": "^5.4.6",
    "node-rsa": "^1.0.3",
    "node-xss": "0.0.1",
    "pug": "^2.0.0-rc.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.8.1"
  }
}
